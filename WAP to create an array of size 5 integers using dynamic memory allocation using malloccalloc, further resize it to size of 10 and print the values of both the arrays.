#include <iostream>
#include <algorithm>

using namespace std;

int main() 
{
  int* arr = (int*) malloc(5 * sizeof(int));

  for(int i=0;i<5;i++)
    {
      cout << "Enter number " <<i+1<< ": ";
      cin >> arr[i];
      
    }
  
  cout<< "Elements of the array are: "<<endl;
  for(int i=0;i<5;i++)
    {
      cout<< arr[i]<< " ";
    }

  int* arr1 = (int*) malloc(10 * sizeof(int));
  
}
